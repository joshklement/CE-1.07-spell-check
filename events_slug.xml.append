<event name="BS_EVENT_SLUG_BLACKMARKET_NEBULA"> <!-- neutral -->
	<text load="BS_EVENT_SLUG_BLACKMARKET_TEXT"/>
	<ship load="BS_SLUG" hostile="false"/>
	<environment type="nebula"/>
	<choice>
		<text>Continue...</text>
	<event>
	<text>You consider dealing with the station.</text>
